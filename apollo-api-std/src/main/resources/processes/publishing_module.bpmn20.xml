    <sequenceFlow id="flow8" sourceRef="sendNotificationOnFeedback" targetRef="endEvent"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="decisionOnKS" targetRef="sendNotificationOfDraft">
