  <process id="sd_publishing" name="Publishing module" isExecutable="true">
    <startEvent id="startEvent" flowable:formFieldValidation="true"></startEvent>
    <userTask id="submitDraft" name="Submit draft for editing" flowable:assignee="${standard_officer}" flowable:formFieldValidation="true">
      <extensionElements>
        <modeler:initiator-can-complete xmlns:modeler="http://flowable.org/modeler"><![CDATA[false]]></modeler:initiator-can-complete>
      </extensionElements>
    </userTask>
    <serviceTask id="sendNotificationOfStandard" name="Send notification of a standard for editing" flowable:class="apollo.sd.email_handler.kt"></serviceTask>
    <userTask id="reveiewKS" name="Approve or disapprove KS" flowable:candidateGroups="head_of_publishing" flowable:formFieldValidation="true"></userTask>
    <exclusiveGateway id="decisionOnKS"></exclusiveGateway>
    <userTask id="rejectSendFeedback" name="Reject and send feedback" flowable:candidateGroups="head_of_publishing" flowable:formFieldValidation="true"></userTask>
    <userTask id="approveDraftStandard" name="Approved draft standard for editing" flowable:candidateGroups="head_of publishing" flowable:formFieldValidation="true"></userTask>
    <userTask id="rejectionWithReason" name="Receive notification of rejection with reason" flowable:formFieldValidation="true"></userTask>
    <endEvent id="endEvent"></endEvent>
    <serviceTask id="sendNotificationOfDraft" name="Send notification of draft standard to edit" flowable:class="apollo.sd.email_handler.kt"></serviceTask>
    <userTask id="editDraftStandard" name="Edit draft standard" flowable:candidateGroups="editor" flowable:formFieldValidation="true"></userTask>
    <serviceTask id="sendNotificationProofreading" name="Send notification for proofreading" flowable:class="apollo.sd.email_handler.kt"></serviceTask>
    <userTask id="proofRead" name="Proofread" flowable:candidateGroups="proofreader" flowable:formFieldValidation="true"></userTask>
    <exclusiveGateway id="isThereNeedToProofread"></exclusiveGateway>
    <serviceTask id="sendNotificationForDraught" name="Send notification for draughting" flowable:class="apollo.sd.email_handler.kt"></serviceTask>
    <userTask id="draught" name="Draught" flowable:candidateGroups="draughtsman" flowable:formFieldValidation="true"></userTask>
    <userTask id="checkDraughtApproveChange" name="Check draught standard and approve change" flowable:candidateGroups="head_of_publishing" flowable:formFieldValidation="true"></userTask>
    <sequenceFlow id="flow1" sourceRef="startEvent" targetRef="submitDraft"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="submitDraft" targetRef="sendNotificationOfStandard"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="sendNotificationOfStandard" targetRef="reveiewKS"></sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="reveiewKS" targetRef="decisionOnKS"></sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="decisionOnKS" targetRef="rejectSendFeedback">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!approved}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="decisionOnKS" targetRef="approveDraftStandard">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approved}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="approveDraftStandard" targetRef="sendNotificationOfDraft"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="sendNotificationOfDraft" targetRef="editDraftStandard"></sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="editDraftStandard" targetRef="sendNotificationProofreading"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="proofRead" targetRef="isThereNeedToProofread"></sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="isThereNeedToProofread" targetRef="sendNotificationForDraught">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${draught}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow16" sourceRef="sendNotificationForDraught" targetRef="draught"></sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="draught" targetRef="checkDraughtApproveChange"></sequenceFlow>
    <serviceTask id="sendNotificationOnFeedback" name="Send notification" flowable:class="com.apollo.standardsdevelopment.handlers.NotificationHandler"></serviceTask>
    <sequenceFlow id="flow7" sourceRef="sendNotificationOnFeedback" targetRef="rejectionWithReason"></sequenceFlow>
    <sequenceFlow id="flow80" sourceRef="rejectSendFeedback" targetRef="sendNotificationOnFeedback"></sequenceFlow>
    <serviceTask id="receiveNotificationEditedDraft" name="Receive notification of edited draft standard" flowable:class="com.apollo.standardsdevelopment.handlers.NotificationHandler"></serviceTask>
    <sequenceFlow id="flow81" sourceRef="checkDraughtApproveChange" targetRef="receiveNotificationEditedDraft"></sequenceFlow>
    <sequenceFlow id="flow82" sourceRef="receiveNotificationEditedDraft" targetRef="endEvent"></sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="isThereNeedToProofread" targetRef="checkDraughtApproveChange">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!draught}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="rejectionWithReason" targetRef="endEvent"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="sendNotificationProofreading" targetRef="proofRead"></sequenceFlow>
