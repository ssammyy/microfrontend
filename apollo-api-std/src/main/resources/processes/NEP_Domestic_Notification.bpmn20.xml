  <process id="NEPDomesticNotification" name="NEP Domestic Notification" isExecutable="true">
    <documentation>The process for NEP Domestic Notification includes: Receive Notification and save them on the system (Qaimss). NEP officer evaluate the notification and value their relevance to trade, If the notification is relevant to trade, request for approval from Manager Trade affairs, Uploading the enquiry to WTO</documentation>
    <startEvent id="processBegin" flowable:formFieldValidation="true"></startEvent>
    <exclusiveGateway id="acceptReject" name="Accept"></exclusiveGateway>
    <userTask id="draftNotification" name="Draft a notification " flowable:candidateGroups="SD_NEP_OFFICER" flowable:formFieldValidation="true"></userTask>
    <userTask id="receiveNotification" name="Receive Notification for Approval" flowable:candidateGroups="MANAGER" flowable:formFieldValidation="true"></userTask>
    <exclusiveGateway id="approved" name="Approved"></exclusiveGateway>
    <userTask id="uploadNotification" name="Upload to WTO NSS" flowable:candidateGroups="SD_NEP_OFFICER" flowable:formFieldValidation="true"></userTask>
    <endEvent id="sid-579CBC57-D345-4E28-9230-B2B98BEF2501"></endEvent>
    <sequenceFlow id="startSequence" sourceRef="processBegin" targetRef="NEPReceiveNotification"></sequenceFlow>
    <sequenceFlow id="notificationDecisionSequence" sourceRef="NEPReceiveNotification" targetRef="acceptReject"></sequenceFlow>
    <sequenceFlow id="notificationRejectedSequence" name="No" sourceRef="acceptReject" targetRef="sid-579CBC57-D345-4E28-9230-B2B98BEF2501">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!Yes}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="draftNotificationSequence" sourceRef="draftNotification" targetRef="receiveNotification"></sequenceFlow>
    <sequenceFlow id="approvalReceivedSequence" sourceRef="receiveNotification" targetRef="approved"></sequenceFlow>
    <sequenceFlow id="noApprovedSequence" name="No" sourceRef="approved" targetRef="draftNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!Accept}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="yesApprovedSequence" name="Yes" sourceRef="approved" targetRef="uploadNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${Accept}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="uploadedToNssSequence" sourceRef="uploadNotification" targetRef="sid-579CBC57-D345-4E28-9230-B2B98BEF2501"></sequenceFlow>
    <sequenceFlow id="notificationApprovedSequence" name="Yes" sourceRef="acceptReject" targetRef="draftNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${Yes}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="NEPReceiveNotification" name="Accept or Reject Notification" flowable:candidateGroups="SD_NEP_OFFICER" flowable:formFieldValidation="true"></userTask>
  </process>
