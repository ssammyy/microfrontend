{
  "reports": {
    "demand_note_report": {
      "base_query": "DAT_KEBS_CD_DEMAND_NOTE",
      "filters": {
        "date__range": "DATE_GENERATED",
        "station_id": "ENTRY_POINT",
        "status": "PAYMENT_STATUS",
        "importer": "IMPORTER_PIN"
      },
      "fields": {
        "CD_REF_NUMBER": "CD REF",
        "TO_CHAR(DATE_GENERATED, 'DD-MON-YYYY HH24:MI')": "DATE GENERATED",
        "DEMAND_NOTE_NUMBER": "REF NUMBER",
        "ENTRY_POINT": "CFS NAME",
        "ENTRY_NO": "ENTRY NO",
        "IMPORTER_PIN": "IMPORTER PIN",
        "NAME_IMPORTER": "NAME IMPORTER",
        "C_F_VALUE": "CF VALUE",
        "TOTAL_AMOUNT": "AMOUNT",
        "RECEIPT_NO": "DEMAND NOTE NO.",
        "TO_CHAR(RECEIPT_DATE, 'DD-MON-YYYY HH24:MI')": "RECEIPT DATE",
        "(case when PAYMENT_STATUS=1 then 'Yes' else 'No' end) as PAID_STATUS": "PAID"
      }
    },
    "manifest_report": {
      "base_query": "DAT_KEBS_MANIFEST_DETAILS md",
      "filters": {
        "date__range": "md.EXPECTED_DATE",
        "station_id": "md.EXIT_OFFICE_SUB3",
        "manifest": "md.MANIFEST_NUMBER",
        "bill_number": "md.TD_BILL_CODE",
        "importer": "md.CONSIGNEE_PIN_NUMBER"
      },
      "fields": {
        "MANIFEST_NUMBER": "MANIFEST NUMBER",
        "TRANSPORT_ID": "VESSEL NAME",
        "VOYAGE_NUMBER": "VOYAGE NUMBER",
        "EXPECTED_DATE": "DATE OF ARRIVAL",
        "CONSIGNEE_BUSINESS_NAME": "IMPORTER NAME",
        "CONSIGNOR_BUSINESS_NAME": "EXPORTER NAME",
        "GOODS_DESCRIPTION": "PRODUCT NAME",
        "VOLUME": "QUANTITY",
        "CONSIGNEE_BUSINESS_NATION": "COUNTRY OF ORIGIN/SUPPLY",
        "TD_BILL_CODE": "BILL OF LANDING",
        "EXIT_OFFICE_SUB3": "STATION OF CLEARANCE(FINAL DESTINATION)",
        "DISCHARGE_PLACE": "PORT OF DESTINATION",
        "PLACE_OF_LOADING": "STATION OF CLEARANCE",
        "PLACE_OF_DELIVERY": "PORT OF DESTINATION",
        "CONSIGNEE_PIN_NUMBER": "IMPORTER PIN",
        "CONSIGNOR_PIN_NUMBER": "EXPORTER PIN"
      }
    },
    "idf_report": {
      "base_query": "DAT_KEBS_DECLARATION_ITEM_DETAILS did left join DAT_KEBS_DECLARATION_DETAILS dd on(did.DECLARATION_DETAILS_ID=dd.ID) left join DAT_KEBS_CD_ITEM_NON_STANDARD cdns on(did.CHASSIS_NUMBER=cdns.CHASSIS_NO)",
      "filters": {
        "date__range": "dd.CREATED_ON",
        "clearing_agent": "dd.DECLARANT_PIN",
        "idf": "dd.DECLARATION_REF_NO",
        "hs_code": "did.COMMODITY_CODE",
        "exporter": "dd.CONSIGNOR_BUSINESS_NAME",
        "bill_number": "dd.BILL_CODE",
        "importer": "dd.CONSIGNEE_PIN_NUMBER"
      },
      "fields": {
        "dd.DECLARATION_REF_NO": "DECLARATION NUMBER",
        "dd.CONSIGNEE_BUSINESS_NAME": "IMPORTER NAME",
        "dd.CONSIGNOR_BUSINESS_NAME": "EXPORTER NAME",
        "dd.DECLARANT_PIN": "CLEARING AGENT",
        "TO_CHAR(dd.CREATED_ON,'DD-MON-YYYY HH24:MI')": "DATE RECEIVED",
        "TO_CHAR(dd.MESSAGE_DATE,'DD-MON-YYYY HH24:MI')": "DATE OF ARRIVAL",
        "did.DESCRIPTION": "PRODUCT NAME",
        "did.COMMODITY_CODE": "HS CODE",
        "did.CUSTOMS_VALUE": "CUSTOM VALUE",
        "dd.CURRENCY_CODE": "CURRENCY",
        "did.ITEM_NUM": "QUANTITY",
        "dd.DELIV_TERMS_SUB2": "CUSTOM CLEARANCE STATION",
        "did.CHASSIS_NUMBER": "CHASSIS NUMBER",
        "did.ENGINE_NUMBER": "ENGINE NO.",
        "cdns.VEHICLE_MAKE": "MAKE",
        "cdns.VEHICLE_MODEL": "MODEL",
        "cdns.VEHICLE_YEAR": "YEAR",
        "cdns.USED_INDICATOR": "USED IDICATOR"
      }
    },
    "cor_report": {
      "base_query": "DAT_KEBS_CORS_BAK cb",
      "filters": {
        "chassis": "md.CHASIS_NUMBER",
        "country": "md.COUNTRY_OF_SUPPLY",
        "model": "cb.MODEL",
        "doc_type": "cb.DOCUMENT_TYPE",
        "date__range": "cb.COR_ISSUE_DATE"
      },
      "fields": {
        "cb.COR_NUMBER": "COR NO.",
        "cb.UCR_NUMBER": "UCR NO.",
        "TO_CHAR(cb.COR_ISSUE_DATE, 'DD-MON-YYYY HH24:MI') as COR_ISSUE_DATE": "ISSUE DATE",
        "cb.COUNTRY_OF_SUPPLY": "Country of Supply",
        "cb.INSPECTION_CENTER": "Inspection Center",
        "TO_CHAR(cb.APPLICATION_BOOKING_DATE, 'DD-MON-YYYY HH24:MI') as APPLICATION_BOOKING_DATE": "Application Date",
        "TO_CHAR(cb.INSPECTION_DATE, 'DD-MON-YYYY HH24:MI') as INSPECTION_DATE": "Inspection Date",
        "cb.MAKE": "Make",
        "cb.MODEL": "Model",
        "cb.CHASIS_NUMBER": "Chassis No.",
        "cb.ENGINE_NUMBER": "Engine Number",
        "cb.ENGINE_CAPACITY": "Engine Capacity",
        "cb.YEAR_OF_MANUFACTURE": "Year of Manufacture",
        "cb.YEAR_OF_FIRST_REGISTRATION": "Year of First Reg.",
        "cb.INSPECTION_MILEAGE": "Mileage",
        "cb.UNITS_OF_MILEAGE": "Mileage Unit",
        "cb.INSPECTION_REMARKS": "Remarks"
      }
    },
    "coc_report": {
      "base_query": "DAT_KEBS_COC_ITEMS citm left join DAT_KEBS_COCS c on (citm.COC_ID=c.ID)",
      "filters": {
        "hs_code": "citm.SHIPMENT_LINE_HSCODE",
        "doc_type": "c.DOCUMENT_TYPE",
        "country": "c.COUNTRY_OF_SUPPLY",
        "coc": "c.COC_NUMBER",
        "station_id": "c.ISSUING_OFFICE",
        "assigned_to": "cdd.ASSIGNED_INSPECTION_OFFICER",
        "assigned_by": "cdd.ASSIGNER",
        "line_number": "citm.SHIPMENT_LINE_NUMBER",
        "date__range": "c.COC_ISSUE_DATE",
        "coi_date__range": "c.COI_ISSUE_DATE"
      },
      "fields": {
        "c.IDF_NUMBER": "IDF NUMBER",
        "TO_CHAR(c.RFC_DATE, 'DD-MON-YYYY HH24:MI') as RFC_DATE": "RFC DATE",
        "TO_CHAR(c.COC_ISSUE_DATE, 'DD-MON-YYYY HH24:MI') as COC_ISSUE_DATE": "ISSUE DATE",
        "c.COC_NUMBER": "Cert No.",
        "c.IS_CLEAN": "Compliant",
        "case when length(c.COC_REMARKS)>0 then c.COC_REMARKS else c.COI_REMARKS end": "Remarks",
        "c.ISSUING_OFFICE": "Issuing Office",
        "c.ROUTE": "Route",
        "c.UCR_NUMBER": "UCR No.",
        "c.IMPORTER_NAME": "Importer Name",
        "c.IMPORTER_EMAIL": "Importer Email",
        "c.EXPORTER_NAME": "Exporter Name",
        "c.EXPORTER_EMAIL": "Exporter Email",
        "c.PLACE_OF_INSPECTION": "Place of Inspection",
        "c.PORT_OF_DESTINATION": "Port of destination",
        "c.COUNTRY_OF_SUPPLY": "Country of Supply",
        "c.FINAL_INVOICE_FOB_VALUE": "FOB Value",
        "c.FINAL_INVOICE_CURRENCY": "Currency",
        "c.FINAL_INVOICE_NUMBER": "Invoice Number",
        "citm.SHIPMENT_LINE_DESCRIPTION": "Line Description",
        "citm.SHIPMENT_LINE_STANDARDS_REFERENCE": "Shipment Line STD Ref.",
        "citm.SHIPMENT_LINE_QUANTITY": "Shipment Line QTY",
        "citm.SHIPMENT_LINE_UNITOF_MEASURE": "Shipment Line Units",
        "citm.SHIPMENT_LINE_HSCODE": "Shipment Line HsCode",
        "citm.SHIPMENT_LINE_NUMBER": "Shipment Line No.",
        "citm.SHIPMENT_CONTAINER_NUMBER": "Shipment Line Container No."
      }
    },
    "consignment_report": {
      "base_query": "DAT_KEBS_CD_ITEM_DETAILS cid left join DAT_KEBS_CONSIGNMENT_DOCUMENT_DETAILS cdd on (cid.CD_DOC_ID=cdd.ID) left join DAT_KEBS_CD_STANDARDS_TWO cs2 on(cdd.CD_STANDARDS_TWO=cs2.ID)  left join  DAT_KEBS_DECLARATION_DETAILS kdd on (cdd.MANIFEST_NUMBER=kdd.MANIFEST_NUMBER)  left join DAT_KEBS_CD_ITEM_NON_STANDARD cdns on (cdns.CD_ITEM_DETAILS_ID=cid.ID)",
      "filters": {
        "station_id": "cdd.FREIGHT_STATION",
        "doc_type": "cdd.DOC_TYPE_ID",
        "coc_category": "cdd.CD_COC_LOCAL_TYPE_ID",
        "hs_code": "cid.ITEM_HS_CODE",
        "country": "cid.COUNTRY_OF_SUPPLY",
        "compliant": "cdd.COMPLIANT_STATUS",
        "sampled": "cid.SAMPLED_STATUS",
        "coc_type": "cs2.COC_TYPE",
        "importer": "kdd.CONSIGNEE_PIN_NUMBER",
        "exporter": "kdd.CONSIGNOR_PIN_NUMBER",
        "chassis": "cid.CHASIS_NUMBER",
        "verified": "cid.APPROVE_STATUS",
        "assigned_to": "cdd.ASSIGNED_INSPECTION_OFFICER",
        "owner_id": "cdd.ASSIGNED_INSPECTION_OFFICER",
        "assigner": "cdd.ASSIGNER",
        "req_assigner_id": "cdd.ASSIGNER",
        "line_number": "cid.SHIPMENT_LINE_NUMBER",
        "date__range": "cdd.CREATED_ON"
      },
      "fields": {
        "kdd.DECLARATION_REF_NO": "Declaration",
        "cdd.MANIFEST_NUMBER": "Date Application Submitted",
        "kdd.CD_REGISTRATION_DATE": "CD Registration Date",
        "kdd.REF_NUM": "Ref Num",
        "cdd.UCR_NUMBER": "UCR No.",
        "cdd.DESCRIPTION": "Document Name",
        "cs2.COC_TYPE": "Local Coc Type",
        "cs2.COC_REF_NO": "COC Ref No.",
        "cdd.APPROVE_REJECT_CD_DATE": "Approval Date",
        "kdd.CONSIGNEE_BUSINESS_NAME": "Importer Name",
        "kdd.CONSIGNOR_BUSINESS_NAME": "Exporter Name",
        "kdd.SUB_OFFICE_3": "Port Of Destination",
        "kdd.CONSIGNOR_COUNTRY_CODE": "Country of Supply",
        "cid.ITEM_DESCRIPTION": "Item Description",
        "cid.ITEM_HS_CODE": "HS Code",
        "cid.HS_DESCRIPTION": "HS COde Description",
        "cid.QUANTITY": "Item Quantity",
        "cid.UNIT_OF_QUANTITY": "Unit of Quantity",
        "cid.UNIT_PRICE_FCY": "Unit Price",
        "cid.TOTAL_PRICE_FCY": "CIF",
        "cid.CHASIS_NUMBER": "Vehicle Chassis",
        "cdns.VEHICLE_MAKE": "Make",
        "cdns.VEHICLE_MODEL": "Model",
        "cdns.VEHICLE_YEAR": "Vehicle Year",
        "(case when length(cdns.USED_INDICATOR)>0 then 'Used' else 'New' end) as VEHICLE_STATUS": "Vehicle Status",
        "(case when cid.APPROVE_STATUS=1 then 'Y' else 'N' end) as VERIFIED": "Verified",
        "(case when cid.SAMPLED_STATUS=1 then 'Y' else 'N' end) as SAMPLED": "Sampled",
        "cid.SAMPLED_DATE": "Date Sampled",
        "cid.APPROVE_BY": "Approved By",
        "cid.APPROVE_REASON": "Remarks"
      }
    }
  }
}