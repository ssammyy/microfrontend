  <process id="targetConsignmentDocument2" name="Target Consignment Document" isExecutable="true"
           activiti:candidateStarterUsers="supervisor">
    <startEvent id="startTargetConsignmentDocument2" name="Start Targeting"></startEvent>
    <userTask id="supervisorApprove" name="ApproveTargetingSupervisor" activiti:assignee="${supervisor}"
              activiti:candidateUsers="supervisor" activiti:candidateGroups="${cfs_code}"
              activiti:category="${cfs_code}"></userTask>
    <exclusiveGateway id="exclusivegateway1b" name="Exclusive Gateway B"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="supervisorApprove" targetRef="exclusivegateway1b"></sequenceFlow>
    <sequenceFlow id="flow3" name="Approved" sourceRef="exclusivegateway1b"
                  targetRef="targetingSupervisorApprovedStatus">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${taskApproved}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" name="Rejected" sourceRef="exclusivegateway1b"
                  targetRef="updateSupervisorTargetRejectionStatus">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!taskApproved}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="targetingSupervisorApprovedStatus" name="Supervisor Target Consignment"
                 activiti:expression="#{diService.swScheduledTargeting(cdUuid,remarks,supervisor)}"></serviceTask>
    <serviceTask id="updateSupervisorTargetRejectionStatus" name="Update  Supervisor Rejection Status"
                 activiti:expression="#{diService.rejectTargeting(cdUuid,remarks,supervisor)}"></serviceTask>
    <sequenceFlow id="flow6" sourceRef="updateSupervisorTargetRejectionStatus"
                  targetRef="updateConsignmentDocumentProcessFields22"></sequenceFlow>
    <serviceTask id="updateConsignmentDocumentProcessFields22" name="Clear CD Proccess 2" activiti:async="true"
                 activiti:expression="${diService.clearCurrentProcess(cdUuid)}"></serviceTask>
    <endEvent id="endTargetConsignmentDocument2" name="End Targeting"></endEvent>
    <sequenceFlow id="flow8" sourceRef="updateConsignmentDocumentProcessFields22"
                  targetRef="endTargetConsignmentDocument2"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway2a" targetRef="supervisorApprove">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!isSupervisor}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2a" name="Exclusive Gateway A"></exclusiveGateway>
    <sequenceFlow id="flow10" sourceRef="startTargetConsignmentDocument2" targetRef="exclusivegateway2a"></sequenceFlow>
    <sequenceFlow id="flow11" name="Supervisor Targeting" sourceRef="exclusivegateway2a"
                  targetRef="targetingSupervisorApprovedStatus">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isSupervisor}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="targetingSupervisorApprovedStatus"
                  targetRef="updateConsignmentDocumentProcessFields22"></sequenceFlow>
  </process>
