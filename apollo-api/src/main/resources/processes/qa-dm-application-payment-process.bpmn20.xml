        <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="qaDmappCheckDomestic">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isRenewal==0}]]></conditionExpression>
        </sequenceFlow>
        <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow5" sourceRef="qaDmappCheckDomestic" targetRef="exclusivegateway2"></sequenceFlow>
        <userTask id="qaDmappFillDomesticApplication" name="Fill Domestic Application"></userTask>
        <sequenceFlow id="flow6" sourceRef="exclusivegateway2" targetRef="qaDmappFillDomesticApplication">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isDomestic==1}]]></conditionExpression>
        </sequenceFlow>
        <userTask id="qaDmappFillForeignApplication" name="Fill Foreign Application"></userTask>
        <sequenceFlow id="flow7" sourceRef="exclusivegateway2" targetRef="qaDmappFillForeignApplication">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isDomestic==0}]]></conditionExpression>
        </sequenceFlow>
        <userTask id="qaDmappPCMCheckApplicationCompleForeign" name="PCM Check if foreign application complete">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow10" sourceRef="qaDmappPCMCheckApplicationCompleForeign" targetRef="exclusivegateway3"></sequenceFlow>
