            </extensionElements>
        </userTask>
        <userTask id="qaDmappPayment" name="Payment" activiti:category="manufacturer">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow16" sourceRef="qaDmappPayment" targetRef="qaDmappPaymentConfirmation"></sequenceFlow>
        <endEvent id="endevent1" name="End"></endEvent>
        <sequenceFlow id="flow17" sourceRef="qaDmappPaymentConfirmation" targetRef="endevent1"></sequenceFlow>
        <serviceTask id="qaDmappCheckiIfRenewal" name="Check Renewal" activiti:expression="${qualityAssuranceBpmn.qaDmappCheckIfRenewal(permitId)}"></serviceTask>
