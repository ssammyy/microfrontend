        <sequenceFlow id="flow21" sourceRef="exclusivegateway3" targetRef="qaDmappManufacturerCorrectionForeign">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${foreignApplicationComplete==0}]]></conditionExpression>
        </sequenceFlow>
        <serviceTask id="qaDmappPaymentConfirmation" name="Payment Confirmation" activiti:expression="${bpmnNotifications.sendEmailServiceTask(manufacturerEmail)}"></serviceTask>
        <sequenceFlow id="flow22" sourceRef="exclusivegateway1" targetRef="qaDmappPayment">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isRenewal==1}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow24" sourceRef="exclusivegateway3" targetRef="qaDmappPayment">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${foreignApplicationComplete==1}]]></conditionExpression>
        </sequenceFlow>
        <userTask id="qaDmappPCMCheckApplicationCompleDomestic" name="PCM Check Domestic Application Complete"></userTask>
        <sequenceFlow id="flow25" sourceRef="qaDmappFillDomesticApplication" targetRef="qaDmappPCMCheckApplicationCompleDomestic"></sequenceFlow>
        <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow26" sourceRef="qaDmappPCMCheckApplicationCompleDomestic" targetRef="exclusivegateway4"></sequenceFlow>
