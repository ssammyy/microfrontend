        <sequenceFlow id="flow28" sourceRef="exclusivegateway4" targetRef="qaDmappPayment">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${domesticApplicationComplete==1}]]></conditionExpression>
        </sequenceFlow>
