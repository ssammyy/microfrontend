    <process id="qaIIReportingProcess" name="QA Inspection and Reporting Process" isExecutable="true">
        <startEvent id="startevent1" name="Start"></startEvent>
        <userTask id="qaIirInspection" name="QAO Inspection and Form Filling"></userTask>
        <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="qaIirInspection"></sequenceFlow>
        <userTask id="qaIirHofApplicationReview" name="HOF Application Review">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow4" sourceRef="qaIirHofApplicationReview" targetRef="exclusivegateway1"></sequenceFlow>
        <endEvent id="endevent1" name="End">
            <extensionElements>
                <activiti:executionListener event="end" expression="${qualityAssuranceBpmn.endQAIIReportingProcess(objectId)}"></activiti:executionListener>
            </extensionElements>
        </endEvent>
        <sequenceFlow id="flow5" sourceRef="exclusivegateway1" targetRef="qaIirInspection">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvalStatus==0}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow6" sourceRef="exclusivegateway1" targetRef="qaIirDirectorNotif">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvalStatus==1}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow7" sourceRef="qaIirDirectorNotif" targetRef="qaIirCustomerNotif"></sequenceFlow>
        <sequenceFlow id="flow8" sourceRef="qaIirCustomerNotif" targetRef="endevent1"></sequenceFlow>
        <sequenceFlow id="flow9" sourceRef="qaIirInspection" targetRef="qaIirHofApplicationReview"></sequenceFlow>
        <serviceTask id="qaIirDirectorNotif" name="Director Notification" activiti:expression="${bpmnNotifications.sendEmail(execution,162,email)}"></serviceTask>
        <serviceTask id="qaIirCustomerNotif" name="Customer Notification" activiti:expression="${bpmnNotifications.sendEmail(execution,162,customerEmail)}"></serviceTask>
    </process>
