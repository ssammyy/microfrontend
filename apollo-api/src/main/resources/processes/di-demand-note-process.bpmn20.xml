  <process id="demandNoteGenerationProcess" name="Generate Demand Note for Consignment Documet" isExecutable="true" activiti:candidateStarterUsers="supervisor">
    <startEvent id="startDemandNoteGeneration" name="Start Targeting"></startEvent>
    <sequenceFlow id="flow4" name="Rejected" sourceRef="exclusivegateway2" targetRef="updateCocRejectionStatus">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!taskApproved}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="demandNoteApprovedStatus" name="Approve Demand Note" activiti:expression="#{invoiceService.approveDemandNoteGeneration(cdUuid,demandNoteId,supervisor,remarks)}"></serviceTask>
    <serviceTask id="updateCocRejectionStatus" name="Update Demand Note Rejection Status" activiti:expression="#{invoiceService.rejectDemandNoteGeneration(cdUuid,demandNoteId,remarks)}"></serviceTask>
    <sequenceFlow id="flow6" sourceRef="updateCocRejectionStatus" targetRef="updateConsignmentDocumentProcessFields"></sequenceFlow>
    <serviceTask id="updateConsignmentDocumentProcessFields" name="Update Consignment Document" activiti:expression="${diService.clearCurrentProcess(cdUuid)}"></serviceTask>
    <endEvent id="endTargetConsignmentDocument" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="updateConsignmentDocumentProcessFields" targetRef="endTargetConsignmentDocument"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="servicetask1" targetRef="updateConsignmentDocumentProcessFields"></sequenceFlow>
    <userTask id="requestApprovalOfExcludedItems" name="Approve Demand Note Generation" activiti:assignee="${supervisor}" activiti:category="${cfs_code}"></userTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow12" sourceRef="requestApprovalOfExcludedItems" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow13" name="Approved" sourceRef="exclusivegateway2" targetRef="demandNoteApprovedStatus">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${taskApproved}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="startDemandNoteGeneration" targetRef="requestApprovalOfExcludedItems"></sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="demandNoteApprovedStatus" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Send to SW" activiti:expression="#{invoiceService.sendDemandNote(cdUuid,demandNoteId)}"></serviceTask>
  </process>
