  <process id="msComplaintsProcess" name="Market Surveillance Complaints Process" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="msCoSubmitComplaint" name="Submit Complaint"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="msCoSubmitComplaint"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="msCoSubmitComplaint" targetRef="parallelgateway1"></sequenceFlow>
    <parallelGateway id="parallelgateway1" name="Exclusive Gateway"></parallelGateway>
    <serviceTask id="msCoAcknowledgementNotif" name="Acknowledgement Notif" activiti:expression="${bpmnNotifications.sendEmail(execution,127,customerEmail)}"></serviceTask>
    <sequenceFlow id="flow3" sourceRef="parallelgateway1" targetRef="msCoAcknowledgementNotif"></sequenceFlow>
    <userTask id="msCoAcceptComplaint" name="HOD Accept Complaint">
      <extensionElements>
        <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow4" sourceRef="parallelgateway1" targetRef="msCoAcceptComplaint"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="msCoAcceptComplaint" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="msCoAssignMsIO" name="Assign MS-IO">
      <extensionElements>
        <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow6" sourceRef="exclusivegateway1" targetRef="msCoAssignMsIO">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${complaintAccepted==1}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="msCoMSProcess" name="MS Process" activiti:async="true" activiti:exclusive="false">
      <extensionElements>
        <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow7" sourceRef="msCoAssignMsIO" targetRef="msCoMSProcess"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="msCoMSProcess" targetRef="endevent1"></sequenceFlow>
    <serviceTask id="msCoRejectionNotif" name="Rejection Notification" activiti:expression="${bpmnNotifications.sendEmail(execution,121,customerEmail)}"></serviceTask>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway1" targetRef="msCoRejectionNotif">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${complaintAccepted==0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="msCoRejectionNotif" targetRef="endevent1"></sequenceFlow>
    <boundaryEvent id="timerMSOfficer" name="Timer" attachedToRef="msCoMSProcess" cancelActivity="false">
      <extensionElements>
        <activiti:executionListener event="end" expression="${bpmnNotifications.sendEmail()}"></activiti:executionListener>
      </extensionElements>
      <timerEventDefinition>
        <timeDuration>PT4S</timeDuration>
      </timerEventDefinition>
    </boundaryEvent>
  </process>
