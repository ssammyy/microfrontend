  <process id="updateConsignmentStatus" name="Approve Consignment Document" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" name="Rejected" sourceRef="exclusivegateway1" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression">
        <![CDATA[${!consignmentDocumentApproved}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${consignmentDocumentApproved}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask1" name="Check Has Coc/Cor" activiti:exclusive="false"
                 activiti:expression="#{diService.consignmentChecks(cdUuid)}"></serviceTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="servicetask1" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow6" name="No CoC/Cor" sourceRef="exclusivegateway2" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!cocCorGenerated}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="servicetask2" targetRef="servicetask5"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="servicetask3" targetRef="servicetask4"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="servicetask5" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow11" name="Has Cor/Coc" sourceRef="exclusivegateway2" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${cocCorGenerated}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask2" name="Reject Approval"
                 activiti:expression="#{diService.consignmentChecks(cdUuid)}"></serviceTask>
    <sequenceFlow id="flow12" sourceRef="startevent1" targetRef="exclusivegateway1"></sequenceFlow>
    <serviceTask id="servicetask3" name="Update CD Status" activiti:exclusive="false"
                 activiti:expression="#{diService.updateStatus(cdUuid,cdStatusTypeId,supervisor,remarks)}"></serviceTask>
    <serviceTask id="servicetask4" name="Update SW"
                 activiti:expression="#{diService.updateCdStatusOnSw(cdUuid,cdStatusTypeId,remarks)}"></serviceTask>
    <sequenceFlow id="flow13" sourceRef="servicetask4" targetRef="servicetask5"></sequenceFlow>
    <serviceTask id="servicetask5" name="Clear CD Process"
                 activiti:expression="#{diService.clearCurrentProcess(cdUuid)}"></serviceTask>
  </process>
