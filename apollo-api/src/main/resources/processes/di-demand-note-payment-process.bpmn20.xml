<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:activiti="http://activiti.org/bpmn"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
             typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath"
             targetNamespace="http://www.activiti.org/test">
    <process id="demandNotePaymentProcess" name="My process" isExecutable="true">
        <startEvent id="startevent1" name="Start"></startEvent>
        <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="updatePaymentOnKentrade"></sequenceFlow>
        <serviceTask id="updatePaymentOnKentrade" name="Send Status to KENTRADE"
                     activiti:expression="#{invoiceService.invoicePaymentCompleted(cdId,demandNoteId,null)}"></serviceTask>
        <endEvent id="paymentCompleted" name="End"></endEvent>
        <sequenceFlow id="flow2" sourceRef="demandNoteSwStatus" targetRef="paymentCompleted"></sequenceFlow>
        <sequenceFlow id="flow3" sourceRef="updatePaymentOnKentrade" targetRef="demandNoteSwStatus"></sequenceFlow>
        <serviceTask id="demandNoteSwStatus" name="Update Local Status"
                     activiti:expression="#{invoiceService.sendDemandNoteStatus(demandNoteId)}"></serviceTask>
    </process>
    <bpmndi:BPMNDiagram id="BPMNDiagram_demandNotePaymentProcess">
        <bpmndi:BPMNPlane bpmnElement="demandNotePaymentProcess" id="BPMNPlane_demandNotePaymentProcess">
            <bpmndi:BPMNShape bpmnElement="startevent1" id="BPMNShape_startevent1">
                <omgdc:Bounds height="35.0" width="35.0" x="270.0" y="230.0"></omgdc:Bounds>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape bpmnElement="updatePaymentOnKentrade" id="BPMNShape_updatePaymentOnKentrade">
                <omgdc:Bounds height="55.0" width="105.0" x="350.0" y="220.0"></omgdc:Bounds>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape bpmnElement="paymentCompleted" id="BPMNShape_paymentCompleted">
                <omgdc:Bounds height="35.0" width="35.0" x="750.0" y="230.0"></omgdc:Bounds>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNShape bpmnElement="demandNoteSwStatus" id="BPMNShape_demandNoteSwStatus">
                <omgdc:Bounds height="55.0" width="105.0" x="570.0" y="220.0"></omgdc:Bounds>
            </bpmndi:BPMNShape>
            <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
                <omgdi:waypoint x="305.0" y="247.0"></omgdi:waypoint>
                <omgdi:waypoint x="350.0" y="247.0"></omgdi:waypoint>
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
                <omgdi:waypoint x="675.0" y="247.0"></omgdi:waypoint>
                <omgdi:waypoint x="750.0" y="247.0"></omgdi:waypoint>
            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
                <omgdi:waypoint x="455.0" y="247.0"></omgdi:waypoint>
                <omgdi:waypoint x="570.0" y="247.0"></omgdi:waypoint>
            </bpmndi:BPMNEdge>
        </bpmndi:BPMNPlane>
    </bpmndi:BPMNDiagram>
</definitions>