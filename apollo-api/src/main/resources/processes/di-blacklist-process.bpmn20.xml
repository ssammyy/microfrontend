  <process id="blacklistUser" name="Blacklist a user in consignment document" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" name="Rejected" sourceRef="exclusivegateway1" targetRef="rejectBlacklist">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!taskApproved}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="processLocalBlacklist">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${taskApproved}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="processLocalBlacklist" name="Blacklist User" activiti:expression="#{diService.approveUserBlacklisting(cdUuid,blacklistId,supervisor,remarks)}"></serviceTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="sendBlacklistToSingleWindow" targetRef="servicetask3"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="servicetask3" targetRef="endevent1"></sequenceFlow>
    <serviceTask id="sendBlacklistToSingleWindow" name="Update On Single Window" activiti:expression="#{diService.swUpdateBlacklistStatus(cdUuid,blacklistId,supervisor,remarks)}"></serviceTask>
    <serviceTask id="rejectBlacklist" name="Rejected blacklist" activiti:expression="#{diService.rejectUserBlacklisting(cdUuid,blacklistId,supervisor,remarks)}"></serviceTask>
    <userTask id="requestApproval" name="Approve Balcklist" activiti:assignee="${supervisor}" activiti:category="${cfs_code}"></userTask>
    <sequenceFlow id="flow12" sourceRef="startevent1" targetRef="requestApproval"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="requestApproval" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="processLocalBlacklist" targetRef="sendBlacklistToSingleWindow"></sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="rejectBlacklist" targetRef="servicetask3"></sequenceFlow>
    <serviceTask id="servicetask3" name="Clear CD Process" activiti:expression="#{diService.clearCurrentProcess(cdUuid)}"></serviceTask>
  </process>
