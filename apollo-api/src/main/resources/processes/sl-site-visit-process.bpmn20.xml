    <process id="sLSiteVisitProcess" name="Standards Levy Site Visit" isExecutable="true">
        <startEvent id="sLSvStart" name="Start"></startEvent>
        <userTask id="sLSvQueryManufacturerDetails" name="Query for Manufacturer Details"></userTask>
        <sequenceFlow id="flow1" sourceRef="sLSvStart" targetRef="sLSvQueryManufacturerDetails"></sequenceFlow>
        <userTask id="sLsVScheduleVisit" name="Schedule Visit">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow2" sourceRef="sLSvQueryManufacturerDetails" targetRef="sLsVScheduleVisit"></sequenceFlow>
        <userTask id="sLsVPrepareVisitReport" name="Prepare Visit Report">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <userTask id="sLsVApproveReportAssMgr" name="Ass Mgr approve report">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow4" sourceRef="sLsVPrepareVisitReport" targetRef="sLsVApproveReportAssMgr"></sequenceFlow>
        <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow5" sourceRef="sLsVApproveReportAssMgr" targetRef="exclusivegateway1"></sequenceFlow>
        <userTask id="sLsVApproveReportMgr" name="Mgr approve report">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow6" sourceRef="exclusivegateway1" targetRef="sLsVApproveReportMgr">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedAsstMgr==1}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow7" sourceRef="exclusivegateway1" targetRef="sLsVPrepareVisitReport">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedAsstMgr==0}]]></conditionExpression>
        </sequenceFlow>
        <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow8" sourceRef="sLsVApproveReportMgr" targetRef="exclusivegateway2"></sequenceFlow>
        <userTask id="sLsVDraftFeedbackAndShare" name="Draft Feedback and Share">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow9" sourceRef="exclusivegateway2" targetRef="sLsVDraftFeedbackAndShare">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedMgr==1}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow10" sourceRef="exclusivegateway2" targetRef="sLsVApproveReportAssMgr">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedMgr==0}]]></conditionExpression>
        </sequenceFlow>
        <serviceTask id="sLsVFeedbackNotification" name="Feedback Notification" activiti:expression="${bpmnNotifications.sendEmailServiceTask(manufacturerId,347,true)}"></serviceTask>
        <sequenceFlow id="flow11" sourceRef="sLsVDraftFeedbackAndShare" targetRef="sLsVFeedbackNotification"></sequenceFlow>
        <endEvent id="endevent1" name="End">
            <extensionElements>
                <activiti:executionListener event="end" expression="${standardsLevyBpmn.endSlSiteVisitProcess(objectId)}"></activiti:executionListener>
            </extensionElements>
        </endEvent>
        <sequenceFlow id="flow12" sourceRef="sLsVFeedbackNotification" targetRef="endevent1"></sequenceFlow>
        <sequenceFlow id="flow13" sourceRef="sLsVScheduleVisit" targetRef="sLsVPrepareVisitReport"></sequenceFlow>
    </process>
