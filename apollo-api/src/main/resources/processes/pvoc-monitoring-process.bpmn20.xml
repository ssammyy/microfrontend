    <process id="pvocMonitoringProcess" name="Pvoc Monitoring Process" isExecutable="true">
        <startEvent id="pvocMoStart" name="Start"></startEvent>
        <userTask id="pvocMoGenerateAnalysisReport" name="Generate analysis report">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow1" sourceRef="pvocMoStart" targetRef="pvocMoGenerateAnalysisReport"></sequenceFlow>
        <userTask id="pvocMoRequestForInfo" name="Request for info">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <userTask id="pvocMoRequestForInfoFeedback" name="Request for Info feedback">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <userTask id="pvocMoGenMonitoredCasesAnalysisRpt" name="Generate monitored cases analysis report">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <userTask id="pvocMoMpvocAnalysisRptReview" name="MPVOC analysis report review">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow7" sourceRef="pvocMoGenMonitoredCasesAnalysisRpt" targetRef="pvocMoMpvocAnalysisRptReview"></sequenceFlow>
        <userTask id="pvocMoHodAnalysisRptReview" name="HOD analysis report review">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow8" sourceRef="pvocMoMpvocAnalysisRptReview" targetRef="pvocMoHodAnalysisRptReview"></sequenceFlow>
        <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow9" sourceRef="pvocMoHodAnalysisRptReview" targetRef="exclusivegateway1"></sequenceFlow>
        <userTask id="pvocMoCheckAnalysisReport" name="Check analysis report">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow10" sourceRef="pvocMoGenerateAnalysisReport" targetRef="pvocMoCheckAnalysisReport"></sequenceFlow>
        <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow11" sourceRef="pvocMoCheckAnalysisReport" targetRef="exclusivegateway2"></sequenceFlow>
        <userTask id="pvocMoGeneratePenaltyLetter" name="Generate penalty letter and invoice">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow12" sourceRef="exclusivegateway1" targetRef="pvocMoGeneratePenaltyLetter">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${analysisReportAction==penalty}]]></conditionExpression>
        </sequenceFlow>
        <userTask id="pvocMoSubmitPenaltyLetter" name="Submit penalty letter and invoice">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow13" sourceRef="pvocMoGeneratePenaltyLetter" targetRef="pvocMoSubmitPenaltyLetter"></sequenceFlow>
        <userTask id="pvocMoPenaltyPayment" name="Penalty payment">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow14" sourceRef="pvocMoSubmitPenaltyLetter" targetRef="pvocMoPenaltyPayment"></sequenceFlow>
        <userTask id="pvocMoGenerateWarningLetter" name="Generate warning letter">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow15" sourceRef="exclusivegateway1" targetRef="pvocMoGenerateWarningLetter">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${analysisReportAction==warning}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow16" sourceRef="exclusivegateway2" targetRef="pvocMoRequestForInfo">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${analysisReportOk==0}]]></conditionExpression>
        </sequenceFlow>
        <endEvent id="pvocMoEnd" name="End">
            <extensionElements>
                <activiti:executionListener event="end" expression="${pvocBpmn.endPvocMoProcess(objectId)}"></activiti:executionListener>
            </extensionElements>
        </endEvent>
        <endEvent id="pvocMoEndAnalysisOk" name="End">
            <extensionElements>
                <activiti:executionListener event="end" expression="${pvocBpmn.endPvocMoProcess(objectId)}"></activiti:executionListener>
            </extensionElements>
        </endEvent>
        <sequenceFlow id="flow18" sourceRef="exclusivegateway2" targetRef="pvocMoEndAnalysisOk">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${analysisReportOk==1}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow19" sourceRef="pvocMoRequestForInfo" targetRef="pvocMoRequestForInfoFeedback"></sequenceFlow>
        <sequenceFlow id="flow20" sourceRef="pvocMoRequestForInfoFeedback" targetRef="pvocMoGenMonitoredCasesAnalysisRpt"></sequenceFlow>
        <userTask id="pvocMoCloseCase" name="Close case">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <sequenceFlow id="flow21" sourceRef="exclusivegateway1" targetRef="pvocMoCloseCase">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${analysisReportAction==close}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow22" sourceRef="pvocMoCloseCase" targetRef="pvocMoEnd"></sequenceFlow>
        <serviceTask id="pvocMoWarningLetterNotification" name="Warning letter notification" activiti:expression="${bpmnNotifications.sendEmailServiceTask(pvocAgentId,282)}"></serviceTask>
        <sequenceFlow id="flow23" sourceRef="pvocMoGenerateWarningLetter" targetRef="pvocMoWarningLetterNotification"></sequenceFlow>
        <sequenceFlow id="flow25" sourceRef="pvocMoPenaltyPayment" targetRef="pvocMoCloseCase"></sequenceFlow>
        <sequenceFlow id="flow26" sourceRef="pvocMoWarningLetterNotification" targetRef="pvocMoCloseCase"></sequenceFlow>
    </process>
