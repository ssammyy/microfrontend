    <process id="qaSfPermitAwardProcess" name="QA SF Permit Award Process" isExecutable="true">
        <startEvent id="startevent1" name="Start"></startEvent>
        <userTask id="qaSfpaPscApproval" name="PSC permit approval">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow3" sourceRef="qaSfpaPscApproval" targetRef="exclusivegateway1"></sequenceFlow>
        <userTask id="qaSfpaPCMPermitAward" name="PCM Permit Award">
            <extensionElements>
                <activiti:taskListener event="assignment" expression="${bpmnNotifications.sendEmail(task)}"></activiti:taskListener>
            </extensionElements>
        </userTask>
        <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow6" sourceRef="qaSfpaPCMPermitAward" targetRef="qaSfpaPermitAwardNotif"></sequenceFlow>
        <sequenceFlow id="flow7" sourceRef="qaSfpaPermitAwardNotif" targetRef="exclusivegateway2"></sequenceFlow>
        <sequenceFlow id="flow8" sourceRef="exclusivegateway2" targetRef="qaSfpaSendEpermit">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${permitAwarded==1}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow9" sourceRef="exclusivegateway2" targetRef="qaSfpaDefferalNotif">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${permitAwarded==0}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow10" sourceRef="exclusivegateway1" targetRef="qaSfpaDefferalNotif">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${permitApproved==0}]]></conditionExpression>
        </sequenceFlow>
        <endEvent id="endevent1" name="End">
            <extensionElements>
                <activiti:executionListener event="end" expression="${qualityAssuranceBpmn.endQASFPermitAwardProcess(permitId)}"></activiti:executionListener>
            </extensionElements>
        </endEvent>
        <sequenceFlow id="flow11" sourceRef="qaSfpaSendEpermit" targetRef="endevent1"></sequenceFlow>
        <sequenceFlow id="flow12" sourceRef="qaSfpaDefferalNotif" targetRef="endevent1"></sequenceFlow>
        <sequenceFlow id="flow13" sourceRef="startevent1" targetRef="qaSfpaPscApproval"></sequenceFlow>
        <sequenceFlow id="flow14" sourceRef="exclusivegateway1" targetRef="qaSfpaPCMPermitAward">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${permitApproved==1}]]></conditionExpression>
        </sequenceFlow>
        <serviceTask id="qaSfpaPermitAwardNotif" name="Pemit Award Notification" activiti:expression="${bpmnNotifications.sendEmailServiceTask(email)}"></serviceTask>
        <serviceTask id="qaSfpaDefferalNotif" name="Deferral Notification" activiti:expression="${bpmnNotifications.sendEmailServiceTask(email)}"></serviceTask>
        <serviceTask id="qaSfpaSendEpermit" name="Send E-Permit" activiti:expression="${bpmnNotifications.sendEmailServiceTask(manufacturerEmail)}"></serviceTask>
    </process>
