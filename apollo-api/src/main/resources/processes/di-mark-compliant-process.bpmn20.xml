    <process id="consignmentCompliantApprovalProcess" name="Consignment Documet Compliant Process" isExecutable="true" activiti:candidateStarterUsers="supervisor">
        <startEvent id="startTargetConsignmentDocument" name="Start Targeting"></startEvent>
        <userTask id="usertask1" name="Approve Compliant" activiti:assignee="${supervisor}" activiti:candidateUsers="supervisor" activiti:category="${cfs_code}"></userTask>
        <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow2" sourceRef="exclusivegateway2" targetRef="cocApprovedStatus">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${documentType=='COC'}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow3" name="Approved" sourceRef="exclusivegateway3" targetRef="exclusivegateway2">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${compliantStatus==1}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow4" name="Rejected" sourceRef="exclusivegateway1" targetRef="updateCocRejectionStatus">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!taskApproved}]]></conditionExpression>
        </sequenceFlow>
        <serviceTask id="cocApprovedStatus" name="Generate CoC" activiti:expression="#{diService.swGenerateCoC(cdUuid,remarks,supervisor)}"></serviceTask>
        <serviceTask id="updateCocRejectionStatus" name="Update  Rejection Status" activiti:expression="#{diService.rejectComplianceRequests(cdUuid,supervisor, remarks)}"></serviceTask>
        <sequenceFlow id="flow6" sourceRef="updateCocRejectionStatus" targetRef="updateConsignmentDocumentProcessFields"></sequenceFlow>
        <serviceTask id="updateConsignmentDocumentProcessFields" name="Update Consignment Document" activiti:expression="${diService.clearCurrentProcess(cdUuid)}"></serviceTask>
        <endEvent id="endTargetConsignmentDocument" name="End"></endEvent>
        <sequenceFlow id="flow8" sourceRef="updateConsignmentDocumentProcessFields" targetRef="endTargetConsignmentDocument"></sequenceFlow>
        <sequenceFlow id="flow9" sourceRef="startTargetConsignmentDocument" targetRef="usertask1"></sequenceFlow>
        <sequenceFlow id="flow10" sourceRef="cocApprovedStatus" targetRef="servicetask5"></sequenceFlow>
        <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow11" sourceRef="servicetask3" targetRef="exclusivegateway1"></sequenceFlow>
        <serviceTask id="servicetask2" name="Generate COR" activiti:expression="#{diService.generateCorForDocument(cdUuid,supervisor,remarks)}"></serviceTask>
        <sequenceFlow id="flow14" sourceRef="servicetask5" targetRef="updateConsignmentDocumentProcessFields"></sequenceFlow>
        <sequenceFlow id="flow15" sourceRef="exclusivegateway2" targetRef="servicetask2">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${documentType=='COR'}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow16" sourceRef="usertask1" targetRef="servicetask3"></sequenceFlow>
        <serviceTask id="servicetask3" name="Mark Compliance Status" activiti:expression="${diService.markCompliant(cdUuid,compliantStatus, supervisor,remarks,taskApproved)}"></serviceTask>
        <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
        <sequenceFlow id="flow17" sourceRef="exclusivegateway1" targetRef="exclusivegateway3">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${taskApproved}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow18" sourceRef="exclusivegateway3" targetRef="servicetask5">
            <conditionExpression xsi:type="tFormalExpression"><![CDATA[${compliantStatus!=1}]]></conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="flow19" sourceRef="servicetask2" targetRef="servicetask5"></sequenceFlow>
        <serviceTask id="servicetask5" name="Update SW" activiti:expression="#{diService.swUpdateComplianceStatus(cdUuid,compliantStatus,supervisor,remarks)}"></serviceTask>
    </process>
